{
  "name": "scale-down",
  "runtime": "nodejs4.3",
  "description": "Scales the no. of concurrent SQS processors up & down",
  "customName": false,
  "customRole": false,
  "handler": "functions/scale-down/handler.handler",
  "timeout": 6,
  "memorySize": 512,
  "authorizer": {},
  "custom": {
    "excludePatterns": [
      "node_modules/aws-sdk"
    ]
  },
  "endpoints": [],
  "events": [
    {
      "name": "scale-down",
      "type": "sns",
      "config": {
        "topicName": "scale-down-sqs"
      }
    }
  ],
  "environment": {
    "SERVERLESS_PROJECT": "${project}",
    "SERVERLESS_STAGE": "${stage}",
    "SERVERLESS_REGION": "${region}",
    "SERVERLESS_FUNCTION": "scale-down",
    "TOKENS_TABLE": "${tokensTable}"
  },
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}